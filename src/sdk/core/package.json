{
  "name": "@traf3li/auth-core",
  "version": "1.0.0",
  "description": "Core JavaScript/TypeScript SDK for Traf3li Authentication",
  "author": "Traf3li",
  "license": "MIT",
  "keywords": [
    "traf3li",
    "authentication",
    "auth",
    "sdk",
    "oauth",
    "mfa",
    "passwordless",
    "magic-link",
    "otp",
    "session-management"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.js"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.mjs",
      "require": "./dist/storage.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.mjs",
      "require": "./dist/events.js"
    },
    "./fetch": {
      "types": "./dist/fetch.d.ts",
      "import": "./dist/fetch.mjs",
      "require": "./dist/fetch.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/traf3li/traf3li-backend.git",
    "directory": "src/sdk/core"
  },
  "bugs": {
    "url": "https://github.com/traf3li/traf3li-backend/issues"
  },
  "homepage": "https://github.com/traf3li/traf3li-backend/tree/main/src/sdk/core#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {},
  "dependencies": {},
  "tsup": {
    "entry": [
      "index.ts",
      "client.ts",
      "types.ts",
      "errors.ts",
      "storage.ts",
      "utils.ts",
      "events.ts",
      "fetch.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "splitting": true,
    "sourcemap": true,
    "clean": true,
    "treeshake": true,
    "minify": false,
    "outDir": "dist",
    "target": "es2020",
    "platform": "neutral"
  }
}
